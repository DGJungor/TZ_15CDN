* {
	box-sizing: content-box;
	font-family: Microsoft YaHei;
}

body {
	margin: 0;
	padding: 0;
	overflow-x: hidden;
	box-sizing: unset;
}

.header {
	width: 100%;
	height: 115px;
	position: absolute;
	z-index: 1;
}

.head-main {
	width: 1150px;
	height: 115px;
	margin: 0 auto;
	min-width: 1150px;
}

.navigation {
	width: 777px;
	height: 90px;
	float: left;
}

.nav-bg {
	width: 367px;
	height: 28px;
	background-color: #36648B;
	-moz-opacity: 0.5;
	opacity: 0.5;
	float: right;
	margin-top: 62px;
	margin-left: 410px;
	border-radius: 15px;
	position: absolute;
}

.nav-ul {
	list-style-type: none;
	float: left;
	margin: 65px 0 0 410px;
	padding: 0;
	width: 367px;
	height: 28px;
	text-align: center;
	position: absolute;
}

.nav-ul li {
	float: left;
	width: 113px;
	height: auto;
}

.nav-ul li a {
	text-decoration: none;
	color: #fff;
	font-weight: 700;
	font-size: 15px;
}

.login {
	margin-top: 62px;
	float: right;
	font-size: 14px;
	color: #fff;
}

.login a {
	text-decoration: none;
	color: #fff;
}

.con-description {
	width: 1150px;
	height: 145px;
	padding: 50px 30px 0 30px;
	min-width: 1090px;
	margin: 0 auto;
}

.con-description p {
	font-size: 14px;
	color: #666;
	margin-bottom: 40px;
	line-height: 30px;
}

.con-des-but {
	margin: 0 auto;
	width: 180px;
	height: 28px;
	background-color: #614d08;
	color: #fff;
	font-weight: 700;
	border-radius: 15px;
	text-align: center;
	font-size: 15px;
	padding-top: 6px;
	cursor: pointer;
	transition: all 0.2s ease-in-out 0s;
}


.con-des-but:HOVER {
	background-color: #2ba3d5;
	transition: all 0.2s ease-in-out 0s;
}
.con-des-but:HOVER a{
	color:#fff;
	text-decoration:none;
}
.def-type {
	width: 1150px;
	height: auto;
	min-width: 1150px;
	margin: 0 auto;
}

.def-type .types {
	padding: 0 50px 50px 50px;
	width: 1050px;
	height: 300px;
}

.types-text {
	float: left;
	width: 405px;
	height: 180px;
	padding: 60px;
}

.types-text span {
	font-size: 15px;
	font-weight: 700;
	color: #666;
}

.types-text p {
	font-size: 13px;
	color: #666;
	text-indent: 2em;
	line-height: 22px;
	margin: 0;
}

.types-img {
	width: 405px;
	height: 200px;
	float: left;
	padding: 50px 50px 50px 70px;
	/* background-color: #000; */
}
.types-img img{
	
}
.footer {
	width: 100%;
	height: 316px;
	margin: 0;
	background-color: #2ba3d5;
}

.footer-con {
	width: 1010px;
	height: 266px;
	margin: 0 auto;
	padding-left: 70px;
	padding-right: 70px;
	padding-top: 50px;
}

.footer-con-ul {
	list-style: none;
	margin: 0;
	width: 1010px;
	height: 100px;
}

.footer-con-li {
	float: left;
	width: 240px;
	height: auto;
	font-size: 15px;
	color: #fff;
	font-weight: 700;
}

.footer-con-innerul {
	list-style: none;
	padding: 0;
	margin-top: 10px;
}

.footer-con-innerul li {
	font-size: 14px;
	color: #fff;
	font-weight: 500;
}

.footer-bot {
	width: 600px;
	height: 100px;
	margin: 50px auto 0;
}

.footer-bot-info {
	list-style: none;
	font-size: 14px;
	color: #fff;
	text-align: center;
}